#: daptup:36
msgid "SIGTERM received"
msgstr "SIGTERM recebido"

#: daptup:37
msgid "SIGINT received"
msgstr "SIGINT recebido"

#: daptup:50
#, sh-format
msgid "Cannot read configuration from '$DAPTUP_CONFFILE'."
msgstr "Não pode ler a configuração a partir de '$DAPTUP_CONFFILE'."

#: daptup:64
#, sh-format
msgid "Usage: $BASENAME [ -h | --help | --nocolor ]."
msgstr "Utilização: $BASENAME [ -h | --help | --nocolor ]."

#: daptup:66
msgid "daptup runs 'aptitude update' command inside and outputs:"
msgstr "O daptup corre o comando 'aptitude update' e mostra:"

#: daptup:67
msgid " - list of packages recently entered to repo;"
msgstr " - lista de pacotes que entraram recentemente no repo;"

#: daptup:68
msgid " - list of packages which got new updates;"
msgstr " - lista de pacotes que obtiveram novas actualizações;"

#: daptup:69
msgid " - list of changes in 'watched' packages;"
msgstr " - lista de alterações em pacotes vigiados;"

#: daptup:70
msgid " - list of outdated packages (optionally)."
msgstr " - lista de pacotes desactualizados (opcional)."

#: daptup:72
msgid "Options:"
msgstr "Opções:"

#: daptup:73
msgid " -h, --help: output this help and exit"
msgstr " -h, --help: mostrar esta ajuda e terminar"

#: daptup:74
msgid " --nocolor: do not use colored output"
msgstr " --nocolor: não mostrar cores"

#: daptup:79
#, sh-format
msgid "Unknown param: $PARAM"
msgstr "Parâmetro desconhecido: $PARAM"

#: daptup:88
msgid "You must run daptup with root privileges."
msgstr "Tem de correr o daptup com privilégios de root."

#: daptup:150
#, sh-format
msgid "warning: cannot find any changelog for package '$package'"
msgstr "aviso: não foi possível encontrar qualquer changelog para o pacote '$package'"

#: daptup:169
msgid "day"
msgid_plural "days"
msgstr[0] "dia"
msgstr[1] "dias"

#: daptup:173
#, sh-format
msgid "error: cannot extract last modification date for package '$package'"
msgstr "erro: não pode extrair a última data de modificação para o pacote '$package'"

#: daptup:176
#, sh-format
msgid "error: cannot fetch last entry from changelog for package '$package'"
msgstr "erro: não pode obter a última entrada do 'changelog' para o pacote '$package'"

#: daptup:184
msgid "[done]"
msgstr "[terminado]"

#: daptup:203
msgid "Building old list of packages... "
msgstr "A construir a antiga lista de pacotes... "

#: daptup:208
msgid "errors present. Is apt/dpkg running?.."
msgstr "ocorreram erros. O apt/dpkg está a correr?.."

#: daptup:213
msgid "Building old list of available updates... "
msgstr "A construir a antiga lista de actualizações disponíveis... "

#: daptup:217
msgid "Building old list of watched packages... "
msgstr "A construir a antiga lista de pacotes vigiados... "

#: daptup:221
msgid "aptitude returned non-zero code, daptup stopped here."
msgstr "o aptitude retornou um código diferente de zero, o daptup parou aqui."

#: daptup:223
msgid "Building new list of packages... "
msgstr "A construir a nova lista de pacotes... "

#: daptup:227
msgid "Building new list of available updates... "
msgstr "A construir a nova lista de pacotes disponíveis... "

#: daptup:231
msgid "Building new list of watched packages... "
msgstr "A construir a nova lista de pacotes vigiados... "

#: daptup:244
msgid "Building list of outdated packages... "
msgstr "A construir a lista de pacotes desactualizados... "

#: daptup:249
msgid ""
"error: DAPTUP_MINIMAL_DAY_COUNT_TREATING_OUTDATED contains non-numeric data"
msgstr ""
"erro: DAPTUP_MINIMAL_DAY_COUNT_TREATING_OUTDATED contém dados não-numéricos"

#: daptup:252
msgid "Skipping check for outdated packages."
msgstr "A saltar a verificação de pacotes desactualizados."

#: daptup:264
msgid "No new updates."
msgstr "Sem novas actualizações."

#: daptup:266
msgid "New updates:"
msgstr "Novas actualizações:"

#: daptup:283
msgid "No new or removed packages."
msgstr "Sem pacotes novos ou removidos."

#: daptup:285
msgid "New and removed packages:"
msgstr "Pacotes novos e removidos:"

#: daptup:302
msgid "No news in watched packages."
msgstr "Sem novidades nos pacotes vigiados."

#: daptup:304
msgid "Changes in watched packages:"
msgstr "Alterações nos pacotes vigiados:"

#: daptup:321
msgid "No outdated packages."
msgstr "Sem pacotes desactualizados."

#: daptup:323
msgid "Outdated packages:"
msgstr "Pacotes desactualizados:"
