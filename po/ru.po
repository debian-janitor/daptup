#: daptup:36
msgid "SIGTERM received"
msgstr "получен сигнал SIGTERM"

#: daptup:37
msgid "SIGINT received"
msgstr "получен сигнал SIGINT"

#: daptup:50
#, sh-format
msgid "Cannot read configuration from '$DAPTUP_CONFFILE'."
msgstr "Не могу прочитать конфигурацию из '$DAPTUP_CONFFILE'"

#: daptup:64
#, sh-format
msgid "Usage: $BASENAME [ -h | --help | --nocolor ]."
msgstr "Использование: $BASENAME [ -h | --help | --nocolor]."

#: daptup:66
msgid "daptup runs 'aptitude update' command inside and outputs:"
msgstr "daptup запускает команду 'aptitude update' внутри себя и выводит:"

#: daptup:67
msgid " - list of packages recently entered to repo;"
msgstr " - список пакетов, пришедших в репозиторий;"

#: daptup:68
msgid " - list of packages which got new updates;"
msgstr " - список пакетов, получивших новые обновления;"

#: daptup:69
msgid " - list of changes in 'watched' packages;"
msgstr " - список изменений в 'наблюдаемых' пакетах;"

#: daptup:70
msgid " - list of outdated packages (optionally)."
msgstr " - список давно не обновлявшихся пакетов (необязательно)."

#: daptup:72
msgid "Options:"
msgstr "Параметры:"

#: daptup:73
msgid " -h, --help: output this help and exit"
msgstr " -h, --help: вывести эту справку и выйти"

#: daptup:74
msgid " --nocolor: do not use colored output"
msgstr " --nocolor: не использовать цветной вывод"

#: daptup:79
#, sh-format
msgid "Unknown param: $PARAM"
msgstr "Неизвестнй параметр: $PARAM"

#: daptup:88
msgid "You must run daptup with root privileges."
msgstr "Вы должны запускать daptup с привилегиями суперпользователя."

#: daptup:150
#, sh-format
msgid "warning: cannot find any changelog for package '$package'"
msgstr "предупреждение: не могу найти список изменений для пакета '$package'"

#: daptup:169
msgid "day old"
msgid_plural "days old"
msgstr[0] "день прошёл"
msgstr[1] "дня прошло"
msgstr[2] "дней прошло"

#: daptup:173
#, sh-format
msgid "error: cannot extract last modification date for package '$package'"
msgstr "ошибка: не могу извлечь последнюю дату модификации для пакета '$package'"

#: daptup:176
#, sh-format
msgid "error: cannot fetch last entry from changelog for package '$package'"
msgstr "ошибка: не могу найти последнюю запись в списке изменений для пакета '$package'"

#: daptup:184
msgid "[done]"
msgstr "[готово]"

#: daptup:203
msgid "Building old list of packages... "
msgstr "Построение старого списка пакетов... "

#: daptup:213
msgid "Building old list of available updates... "
msgstr "Построение старого списка доступных обновлений... "

#: daptup:217
msgid "Building old list of watched packages... "
msgstr "Построение старого списка наблюдаемых пакетов... "

#: daptup:223
msgid "Building new list of packages... "
msgstr "Построение нового списка пакетов... "

#: daptup:227
msgid "Building new list of available updates... "
msgstr "Построение нового списка доступных обновлений... "

#: daptup:231
msgid "Building new list of watched packages... "
msgstr "Построение нового списка наблюдаемых пакетов... "

#: daptup:244
msgid "Building list of outdated packages... "
msgstr "Построение списка давно не обновлявшихся пакетов... "

#: daptup:249
msgid ""
"error: DAPTUP_MINIMAL_DAY_COUNT_TREATING_OUTDATED contains non-numeric data"
msgstr "Переменная DAPTUP_MINIMAL_DAY_COUNT_TREATING_OUTDATED содержит нечисловые данные"

#: daptup:252
msgid "Skipping check for outdated packages."
msgstr "Пропускаю проверку на давно не обновлявшиеся пакеты."

#: daptup:264
msgid "No new updates."
msgstr "Нет новых обновлений."

#: daptup:266
msgid "New updates:"
msgstr "Новые обновления:"

#: daptup:283
msgid "No new or removed packages."
msgstr "Нет новых или удалённых пакетов."

#: daptup:285
msgid "New and removed packages:"
msgstr "Новые и удалённые пакеты:"

#: daptup:302
msgid "No news in watched packages."
msgstr "Нет новостей в наблюдаемых пакетах."

#: daptup:304
msgid "Changes in watched packages:"
msgstr "Изменения в наблюдаемых пакетах:"

#: daptup:321
msgid "No outdated packages."
msgstr "Нет давно не обновлявшихся пакетов."

#: daptup:323
msgid "Outdated packages:"
msgstr "Давно не обновлявшиеся пакеты:"
